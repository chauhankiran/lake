<%- include("shared/header") %>

<div class="container my-4">
  <div class="row">
    <div class="col-md-12">
      <h1 class="mb-3">Dashboard</h1>

      <ul class="list-group">
        <% assignedIssues.forEach(function(assignedIssue) { %>
          <li class="list-group-item">
            <h4>
              <a href="/issues/<%= assignedIssue.id %>">
                <%= assignedIssue.title %></h4>
              </a>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
</div>

<%- include("shared/footer") %>
